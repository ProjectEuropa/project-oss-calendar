    "@nuxtjs/auth": "^4.8.4",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
